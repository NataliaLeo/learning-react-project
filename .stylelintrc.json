{
	"extends": "stylelint-config-standard-scss",
	"rules": {
		"selector-class-pattern": null,
		"scss/double-slash-comment-empty-line-before": null,
		"property-no-vendor-prefix": [
			true,
			{
				"severity": "warning",
				"ignoreSelectors": ["/@-webkit-.*/", "/-webkit-.*/"]
			}
		],
		"at-rule-no-vendor-prefix": [
			true,
			{
				"severity": "warning"
			}
		]
	}
}
